<div>
  <%= form_for [@product] do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>
    <%= f.label :images %>
    <%= f.file_field :images, multiple: true %><br>
    <%= f.label :quantity %>
    <%= f.text_field :quantity %><br>
    <%= f.label :price %>
    <%= f.text_field :price %><br>
    <%= f.label :description %>
    <%= f.text_area :description %><br>
    <%= f.submit %>
  <% end %>
</div>
